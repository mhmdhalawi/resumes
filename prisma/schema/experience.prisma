model Experience {
    id          Int      @id @default(autoincrement())
    company     String
    position    String
    startYear   Int
    endYear     Int
    description String
    resume      Resume   @relation(fields: [resumeId], references: [id])
    resumeId    Int
    created_at  DateTime @default(now())
    updated_at  DateTime @updatedAt

    @@map("experiences")
}
