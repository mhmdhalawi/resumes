model User {
    id           String         @id @default(uuid())
    email        String         @unique
    name         String
    password     String
    created_at   DateTime       @default(now())
    role         Role           @default(USER)
    status       Status         @default(INACTIVE)
    resume       Resume[]
    ActivityLogs ActivityLogs[]

    @@map("users")
}

enum Role {
    USER
    ADMIN
}

enum Status {
    ACTIVE
    INACTIVE
    SUSPENDED
}
